                    <!-- JavaScript for handling language selection -->
                    <script>
                        const greetings = document.getElementById('greetings');
                        const languageSelect = document.getElementById('language-select');

                        languageSelect.addEventListener('change', function () {
                            if (languageSelect.value === 'en') {
                                greetings.textContent = 'Hello!';
                            } else if (languageSelect.value === 'bg') {
                                greetings.textContent = 'Здравейте!';
                            } else if (languageSelect.value === 'fr') {
                                greetings.textContent = 'Bonjour!';
                            } else if (languageSelect.value === 'de') {
                                greetings.textContent = 'Guten Tag!';
                            }
                        });
                    </script>

